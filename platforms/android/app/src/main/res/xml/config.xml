<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dbangel.beta" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FirebaseMessaging">
        <param name="android-package" value="by.chemerisuk.cordova.firebase.FirebaseMessagingPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>DBANGEL</name>
    <description>
        Platform photo sharing eksklusif antara kamu dan angel
    </description>
    <author email="contact@dbangel.asia" href="https://dbangel.asia">
        DBAngel Asia
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="./www/icon-louncer.png" />
    <resource-file src="google-services.json" target="app/google-services.json" />
    <preference name="loglevel" value="DEBUG" />
</widget>
